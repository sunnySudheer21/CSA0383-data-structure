#include<stdio.h>n
int main(){
int array[100],i,number,search;
printf("enter the number of elements");
scanf("%d",&number);
printf("enter %d numbers:",number);
for(i=0;i<number;i++)
{
scanf("%d",&array[i]);
}
printf("enter the element to search:");
scanf("%d",&search);
for(i=0;i<number;i++){
if(array[i]==search){
printf("%d is present in the location %d",search,i+1);
break;
}
}
if(i==number){
printf("%d is not present in array",search);
}
return 0;
}
